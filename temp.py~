#!/usr/bin/python
import SocketServer
import os
import os.path
import StringIO
import re
from os.path import exists


base = "www"
print("www exists --> : ")
print(os.path.exists(base))

base = "www/"
print("www/ exists --> : ")
print(os.path.exists(base))

base = "/www/"
print("/www/ exists --> : ")
print(os.path.exists(base))

base = "/www"
print("/www exists --> : ")
print(os.path.exists(base))


